{
  "name": "cra1-storybook",
  "version": "0.0.1",
  "description": "Project for the merging of storybook 4 with CRA1",
  "main": "index.js",
  "scripts": {
    "copy": "npm run copy-css && npm run copy-png",
    "copy-css": "cpx \"./src/**/*.css\" ./dist",
    "copy-png": "cpx \"./src/**/*.png\" ./dist",
    "build": "tsc && npm run lint && npm run copy && npm run build-storybook",
    "lint": "tslint -p .",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amittkSharma/cra1-storybook.git"
  },
  "keywords": [
    "storybook4.0",
    "CRA1"
  ],
  "author": "AMIT SHARMA",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amittkSharma/cra1-storybook/issues"
  },
  "homepage": "https://github.com/amittkSharma/cra1-storybook#readme",
  "dependencies": {
    "antd": "3.10.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@sambego/storybook-state": "1.3.1",
    "@storybook/addon-actions": "4.0.12",
    "@storybook/addon-console": "1.1.0",
    "@storybook/addon-info": "4.0.12",
    "@storybook/addon-knobs": "4.0.12",
    "@storybook/addon-notes": "4.0.12",
    "@storybook/addon-options": "4.0.12",
    "@storybook/react": "4.0.12",
    "@types/react": "16.7.13",
    "@types/react-dom": "16.0.11",
    "@types/react-router": "4.0.30",
    "@types/react-router-dom": "4.3.0",
    "@types/storybook__addon-actions": "3.4.1",
    "@types/storybook__addon-info": "3.4.2",
    "@types/storybook__addon-knobs": "3.4.1",
    "@types/storybook__addon-notes": "3.3.3",
    "@types/storybook__react": "4.0.0",
    "@types/yup": "0.24.9",
    "babel-loader": "^8.0.4",
    "concurrently": "3.6.0",
    "cpx": "1.5.0",
    "fork-ts-checker-webpack-plugin": "0.4.9",
    "html-loader": "0.5.5",
    "http-proxy-middleware": "0.18.0",
    "markdown-loader": "4.0.0",
    "react-docgen-typescript-loader": "3.0.0",
    "storybook-addon-apollo-graphql": "2.1.0",
    "storybook-readme": "4.0.2",
    "style-loader": "0.23.0",
    "thread-loader": "1.2.0",
    "ts-loader": "4.5.0",
    "tslint": "5.11.0",
    "tslint-checker": "4.1.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "3.0.3",
    "vrsource-tslint-rules": "^6.0.0"
  }
}
